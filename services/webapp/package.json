{
  "name": "webapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "start:dev:app": "react-app-rewired start",
    "start:dev:api": "NODE_ENV=development SSR=true nodemon --watch ssr -e js server",
    "start:dev": "yarn && ./node_modules/.bin/npm-run-all --parallel start:dev:*",
    "start:prod:api": "NODE_ENV=production SSR=true node server",
    "start:prod": "yarn && yarn build && yarn start:prod:api",
    "build": "react-app-rewired build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "serve": "./node_modules/.bin/serve -s build -p 8080",
    "test": "react-app-rewired test --env=jsdom",
    "test:api": "./node_modules/mocha/bin/mocha ./ssr/**/*.test.js* --watch",
    "eject": "react-app-rewired eject"
  },
  "dependencies": {
    "@marcopeg/utils": "^1.2.0",
    "color": "^3.0.0",
    "radium": "^0.24.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.11.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^5.1.1",
    "node-env-file": "^0.1.8",
    "nodemon": "^1.17.3",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-inline-source": "^2.0.0",
    "react-app-rewire-stylus-modules": "^0.0.2",
    "react-app-rewired": "^1.5.0",
    "serve": "^6.5.3",
    "source-map-explorer": "^1.5.0",
    "winston": "^2.4.1",
    "yarn-run-all": "^3.1.1"
  },
  "proxy": "http://localhost:8080"
}
